{
  "name": "ng-tailwind-boilerplate",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start-hot": "ng serve --progress --hmr",
    "start-hot-live": "ng serve --progress --hmr --host 0.0.0.0",
    "build": "ng build --prod --extractCss=true --vendorChunk=true",
    "i18n:init": "ngx-translate-extract --input ./src --output ./src/assets/i18n/template.json --key-as-default-value --replace --format json",
    "i18n:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/en.json --clean --format json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.9",
    "@angular/common": "~9.1.9",
    "@angular/compiler": "~9.1.9",
    "@angular/core": "~9.1.9",
    "@angular/fire": "^6.0.0-rc.2",
    "@angular/forms": "~9.1.9",
    "@angular/platform-browser": "~9.1.9",
    "@angular/platform-browser-dynamic": "~9.1.9",
    "@angular/router": "~9.1.9",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^4.0.0",
    "await-to-js": "^2.1.1",
    "firebase": "^7.14.6",
    "hammerjs": "^2.0.8",
    "lottie-web": "^5.6.10",
    "ngx-device-detector": "^1.4.4",
    "ngx-logger": "^4.1.9",
    "ngx-lottie": "^6.0.0",
    "ngx-toastr": "^12.1.0",
    "rxjs": "~6.5.4",
    "tailwindcss": "^1.4.6",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^9.1.0",
    "@angular-devkit/architect": "~0.900",
    "@angular-devkit/build-angular": "^0.901.7",
    "@angular-devkit/build-ng-packagr": "^0.901.7",
    "@angular/cli": "~9.1.7",
    "@angular/compiler-cli": "~9.1.9",
    "@angularclass/hmr": "^2.1.3",
    "@biesbjerg/ngx-translate-extract": "^7.0.0",
    "@fullhuman/postcss-purgecss": "^2.2.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.43",
    "codelyzer": "^5.1.2",
    "firebase-tools": "^7.12.1",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^9.1.5",
    "postcss-loader": "^3.0.0",
    "postcss-scss": "^2.1.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tsickle": "^0.38.1",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  }
